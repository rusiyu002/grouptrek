<div class="page-header">
  <h3>Interested #<%= @interested.id %></h3>
</div>

<div class="row">
  <div class="col-md-12 mb-2">
    <dl class="dl-horizontal">
      <dt>User</dt>
      <dd><%= @interested.user_id %></dd>

      <dt>Trek</dt>
      <dd><%= @interested.trek_id %></dd>

    </dl>

    <div class="btn-group btn-group-justified">
      <a href="/interesteds" class="btn btn-primary">
        Back
      </a>
      <a href="/interesteds/<%= @interested.id %>/edit" class="btn btn-warning">
        Edit
      </a>
      <a href="/delete_interested/<%= @interested.id %>" class="btn btn-danger" rel="nofollow">
        Delete
      </a>
    </div>
  </div>
</div>
